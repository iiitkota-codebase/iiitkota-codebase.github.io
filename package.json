{
  "private": true,
  "name": "iiitkota-codebase.github.io",
  "homepage": "https://codebase.iiitkota.tech/",
  "version": "0.1.0",
  "scripts": {
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "optionalDependencies": {
    "popper.js": "^1.16.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-scroll": "^1.8.1",
    "react-waypoint": "^9.0.3"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "prettier-plugin-import-sort": "^0.0.6",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "svgo": "^1.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss",
      "git add"
    ],
    "*.svg": [
      "svgo",
      "git add"
    ]
  }
}
